{
    "name": "euclidean-sequencer",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@types/howler": "^2.2.4",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.178",
        "@types/node": "^12.20.29",
        "@types/react": "^17.0.29",
        "@types/react-dom": "^17.0.9",
        "@types/uuid": "^8.3.1",
        "autoprefixer": "^10.4.2",
        "concurrently": "^7.0.0",
        "euclidean-rhythms": "^2.0.1",
        "howler": "^2.2.3",
        "lodash": "^4.17.21",
        "postcss": "^8.4.6",
        "postcss-cli": "^9.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "recoil": "^0.4.1",
        "serve": "^12.0.1",
        "tailwindcss": "^3.0.18",
        "typescript": "^4.4.3",
        "uuid": "^8.3.2"
    },
    "scripts": {
        "watch:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/output.css -w",
        "build:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/output.css",
        "start": "concurrently -n react,tailwind -c blue,cyan \"react-scripts start\" \"npm:watch:tailwind\"",
        "build": "npm run build:tailwind && react-scripts build",
        "serve": "npm run build && npx serve -s build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
